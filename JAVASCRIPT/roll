var student =  { 
		  name : "AMRUTH", 
		  sclass : "10", 
		  rollno : 12345  };
document.writeln(student.name);
document.write("<br>");
document.writeln(student.sclass);
document.writeln("<br>");
document.writeln(student.rollno);
document.writeln("<br>");
document.writeln("<br>");
document.writeln("after deletion");
document.writeln("<br>");
document.writeln("<br>");
		delete student.rollno;
		document.writeln(student.name);
		document.write("<br>");
		document.writeln(student.sclass);
		document.writeln("<br>");
		document.writeln(student.rollno);
